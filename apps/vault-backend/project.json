{
  "name": "vault-backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/vault-backend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "webpack-cli build",
        "args": ["node-env=production"]
      },
      "configurations": {
        "development": {
          "args": ["node-env=development"]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "vault-backend:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "vault-backend:build:development"
        },
        "production": {
          "buildTarget": "vault-backend:build:production"
        }
      }
    },
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "apps/vault-backend"
      }
    },
    "migrate": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "apps/vault-backend"
      }
    },
    "generate-types": {
      "command": "prisma generate",
      "options": {
        "cwd": "apps/vault-backend"
      }
    },
    "seed-db": {
      "command": "npx ts-node prisma/seed.ts",
      "options": {
        "cwd": "apps/vault-backend"
      }
    }
  },
  "tags": []
}
